/* pages/sonofkami/opencs/index.wxss */
page {
  background: #eee;
}

.bg {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.login {
  width: 100%;
  height: 370rpx;
  background: #eee;
}

.loginbox {
  width: 90%;
  display: flex;
  align-items: center;
  padding-top: 38rpx;
  margin: 0 auto;
  position: relative;
}

.headImg {
  height: 110rpx;
  width: 110rpx;
  border-radius: 50%;
  border: 4rpx solid #FFFFFF;
}

.isgetavatar {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}

.djdl {
  padding-left: 25rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #666666;
}

.logininfobox {
  padding-top: 15rpx;
  margin-left: 31rpx;
}

.loginName {
  color: #333333;
  font-weight: 600;
  line-height: 36rpx;
}

.loginphone1 {
  height: 50%;
  color: #666666;
  font-size: 14px;
  padding-top: 19rpx;
  text-align: left;
  padding-left: 0;
  font-weight: 400;
  z-index: 21;
}

.loginphone {
  height: 50%;
  color: #666666;
  font-size: 14px;
  padding-top: 19rpx;
  text-align: left;
  padding-left: 0;
  font-weight: 400;
}

.loginArrow {
  position: absolute;
  width: 60rpx;
  right: 0rpx;
}

.messagenumber {
  position: absolute;
  top: 0rpx;
  left: -15rpx;
  min-width: 36rpx;
  height: 28rpx;
  padding: 0px 2.5rpx;
  line-height: 12rpx;
  background-color: rgb(255, 75, 66);
  border-radius: 15rpx;
  color: #eee;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
}

.numberbox {
  width: 88%;
  height: 150rpx;
  margin: 40rpx auto 0 auto;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #5d88ff;
  border-radius: 10rpx;
}

.numberboxtag {
  text-align: center;
  color: #fff;
}

.whitebox {
  width: 90%;
  border-radius: 10rpx;
  background: #fff;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  box-shadow: 0px 0px 6px 0px rgba(16, 28, 32, 0.14);
}

.whiteboxtitle {
  height: 32rpx;
  width: 90%;
  padding-top: 29rpx;
  margin-left: 33rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  font-weight: 600;
  color: #333333;
  display: flex;
  justify-content: space-between;
}

.more {
  font-size: 26rpx;
  font-weight: 500;
  color: #999;
}

.productService {
  width: 100%;
  margin: 10rpx auto 0 auto;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.productServiceTag {
  width: 25%;
  margin-top: 40rpx;
  text-align: center;
}

.productServicePic image {
  width: 70rpx;
  height: 70rpx;
}

.productServiceText {
  font-size: 24rpx;
  font-weight: 400;
  color: #666666;
}

.targettitle {
  width: 90%;
  margin: 20rpx auto 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.targettitlel {
  display: flex;
  align-items: flex-end;
}

.targettitlel :nth-child(2) {
  font-size: 24rpx;
}

.targettitler {
  font-size: 24rpx;
}

.targetprogress {
  width: 90%;
  margin: 20rpx auto 0 auto;
}

.progressbg {
  width: 100%;
  height: 10rpx;
  background: #eee;
  border-radius: 10rpx;
  display: flex;
  overflow: hidden;
}

.progressone {
  width: 0%;
  height: 100%;
  background: rgb(255, 94, 234);
  border-radius: 10rpx 0 0 10rpx;
  transition: width 2s;
}

.progresstwo {
  width: 0%;
  height: 100%;
  background: rgb(97, 94, 255);
  border-radius: 0 10rpx 10rpx 0;
  transition: width 2s;
}

.targetprogressbottom {
  margin-top: 1rpx;
}

.minitext {
  color: #999;
  font-size: 28rpx;
}

.targetulbox {
  width: 90%;
  height: 180rpx;
  margin: 10rpx auto 0 auto;
  background: rgb(250, 250, 250);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.targetulboxtag {
  width: 90%;
  height: auto;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.targetulboxtag :nth-child(2) {
  font-size: 28rpx;
}

.databox {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: relative;
  margin-top: 30rpx;
  position: relative;
}

.databoxleft {
  text-align: center;
}

.databoxleft :nth-child(1) {
  font-size: 86rpx;
  line-height: 86rpx;
  display: inline-block;
  margin: 0 auto;
}

.databoxleft :nth-child(2) {
  font-size: 36rpx;
  color: hsl(350, 100%, 77%);
  display: inline-block;
  position: absolute;
}

.databoxleft :nth-child(3) {
  font-size: 32rpx;
  color: #999999;
}

.databoxright {
  width: 300rpx;
  height: 250rpx;
  font-size: 26rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.databoxrighttag {
  display: flex;
  justify-content: flex-start;
}

.databoxrighttag :nth-child(1) {
  color: #999;
}

.databoxrighttag :nth-child(2) {
  padding-left: 20rpx;
}

.theline {
  border-top: 1rpx solid #eee;
  width: 90%;
  margin: 30rpx auto 30rpx auto;
}

.productData {
  width: 88%;
  margin: 40rpx auto 0 auto;
  display: flex;
  justify-content: space-between;
}

.productDataTag {
  text-align: center;
}

.productDataNumber {
  font-size: 36rpx;
  font-weight: 500;
  color: #333333;
}

.productDataText {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}

.businessmiddle {
  width: 100%;
  margin-top: 30rpx;
  text-align: center;
}

.businessmiddle :nth-child(1) {
  font-size: 70rpx;
}

.businessmiddle :nth-child(2) {
  font-size: 28rpx;
  color: #333333;
}

.businessbottom {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  text-align: center;
  margin-top: 30rpx;
}