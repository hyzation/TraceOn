::-webkit-scrollbar {
  display: none;
}

.main_container {
  width: 90%;
  position: relative;
  height: 100vh;
  margin: 0 auto;
}

.box {
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.item {
  width: 100%;
  height: 80px;
  border-radius: 10px;
  margin: 0 0 10px;
  opacity: 0.4;
}