/* pages/sonofkami/test11/index.wxss */
.searchbox {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
  background: #fff;
}

.searchboxinput {
  width: 494rpx;
  height: 62rpx;
  background: #F9F9F9;
  line-height: 80rpx;
  position: absolute;
  left: 0;
  display: flex;
  align-items: center;
  border-radius: 30rpx;
  margin-left: 32rpx;
}

.searchboxinputimg {
  width: 26rpx;
  height: 26rpx;
  margin-left: 20rpx;
  margin-right: 13rpx;
}




.return {
  width: 80%;
  height: 100rpx;
  margin: 300rpx auto 0 auto;
  background: #2163F8;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}